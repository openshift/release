{
  "name": "4.17.0-0.nightly-arm64",
  "to": "release-arm64",
  "message": "This release contains OSBS official image builds of all arm64 code in release-4.17 (master) branches, and is updated after those builds are synced to quay.io.",
  "mirrorPrefix": "4.17-art-latest-arm64",
  "expires": "336h",
  "maxUnreadyReleases": 1,
  "minCreationIntervalSeconds": 21600,
  "referenceMode": "source",
  "pullSecretName": "source",
  "overrideCLIImage": "image-registry.openshift-image-registry.svc:5000/ocp/4.17-art-latest:cli",
  "check": {},
  "publish": {
    "tag": {
      "tagRef": {
        "name": "4.17"
      }
    }
  },
  "verify": {
    "aws-control-plane-machine-set-operator-arm64": {
      "optional": true,
      "prowJob": {
        "name": "periodic-ci-openshift-cluster-control-plane-machine-set-operator-release-4.17-periodics-e2e-aws-arm"
      }
    },
    "aws-ovn-arm64": {
      "maxRetries": 3,
      "prowJob": {
        "name": "periodic-ci-openshift-multiarch-master-nightly-4.17-ocp-e2e-aws-ovn-arm64"
      }
    },
    "ovn-upgrade": {
      "maxRetries": 3,
      "prowJob": {
        "name": "periodic-ci-openshift-multiarch-master-nightly-4.17-ocp-e2e-upgrade-aws-ovn-arm64"
      },
      "upgrade": true
    }
  }
}