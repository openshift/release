{
  "name":"4.2.0-0.nightly",
  "endOfLife": true,
  "to": "release",
  "message": "This release contains OSBS official image builds of all code in release-4.2 branches, and is updated after those builds are synced to quay.io.",
  "mirrorPrefix": "4.2-art-latest",
  "expires":"24h",
  "maxUnreadyReleases": 1,
  "minCreationIntervalSeconds": 2400,
  "referenceMode": "source",
  "pullSecretName": "source",
  "check":{
    "OCP and Origin images should match": {
      "consistentImages":{"parent":"4.2.0-0.ci"}
    }
  },
  "publish":{
    "tag":{"tagRef":{"name":"4.2"}}
  },
  "verify":{
    "aws":{
      "maxRetries": 3,
      "prowJob":{"name":"periodic-ci-openshift-release-master-nightly-4.2-e2e-aws"}
    },
    "aws-serial":{
      "maxRetries": 3,
      "prowJob":{"name":"periodic-ci-openshift-release-master-nightly-4.2-e2e-aws-serial"}
    },
    "azure":{
      "optional":true,
      "prowJob":{"name":"periodic-ci-openshift-release-master-nightly-4.2-e2e-azure"}
    },
    "gcp":{
      "optional":true,
      "prowJob":{"name":"periodic-ci-openshift-release-master-nightly-4.2-e2e-gcp"}
    },
    "aws-console":{
      "optional":true,
      "prowJob":{"name":"periodic-ci-openshift-release-master-nightly-4.2-console-aws"}
    },
    "upgrade":{
      "upgrade":true,
      "optional":true,
      "prowJob":{"name":"release-openshift-origin-installer-e2e-aws-upgrade"}
    },
    "upgrade-minor":{
      "upgrade":true,
      "upgradeFrom": "PreviousMinor",
      "optional":true,
      "prowJob":{"name":"release-openshift-origin-installer-e2e-aws-upgrade"}
    }
  }
}
