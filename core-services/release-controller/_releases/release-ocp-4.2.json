{
  "name":"4.2.0-0.nightly",
  "to": "release",
  "message": "This release contains OSBS official image builds of all code in release-4.2 branches, and is updated after those builds are synced to quay.io.",
  "mirrorPrefix": "4.2-art-latest",
  "expires":"168h",
  "maxUnreadyReleases": 2,
  "minCreationIntervalSeconds": 1800,
  "referenceMode": "source",
  "pullSecretName": "source",
  "check":{
    "OCP and Origin images should match": {
      "consistentImages":{"parent":"4.2.0-0.ci"}
    }
  },
  "publish":{
    "tag":{"tagRef":{"name":"4.2"}},
    "bugs":{"verifyBugs":{
      "previousReleaseTag":{
        "namespace":"ocp",
        "name":"release",
        "tag":"4.2.0-rc.0"
      }
    }}
  },
  "verify":{
    "aws":{
      "maxRetries": 3,
      "prowJob":{"name":"release-openshift-ocp-installer-e2e-aws-4.2"}
    },
    "aws-serial":{
      "maxRetries": 3,
      "prowJob":{"name":"release-openshift-ocp-installer-e2e-aws-serial-4.2"}
    },
    "azure":{
      "optional":true,
      "prowJob":{"name":"release-openshift-ocp-installer-e2e-azure-4.2"}
    },
    "azure-serial":{
      "optional":true,
      "prowJob":{"name":"release-openshift-ocp-installer-e2e-azure-serial-4.2"}
    },
    "gcp":{
      "optional":true,
      "prowJob":{"name":"release-openshift-ocp-installer-e2e-gcp-4.2"}
    },
    "gcp-serial":{
      "optional":true,
      "prowJob":{"name":"release-openshift-ocp-installer-e2e-gcp-serial-4.2"}
    },
    "metal":{
      "optional":true,
      "prowJob":{"name":"release-openshift-ocp-installer-e2e-metal-4.2"}
    },
    "metal-serial":{
      "optional":true,
      "prowJob":{"name":"release-openshift-ocp-installer-e2e-metal-serial-4.2"}
    },
    "aws-console":{
      "optional":true,
      "prowJob":{"name":"release-openshift-ocp-installer-console-aws-4.2"}
    },
    "upgrade":{
      "upgrade":true,
      "optional":true,
      "prowJob":{"name":"release-openshift-origin-installer-e2e-aws-upgrade"}
    },
    "upgrade-minor":{
      "upgrade":true,
      "upgradeFrom": "PreviousMinor",
      "optional":true,
      "prowJob":{"name":"release-openshift-origin-installer-e2e-aws-upgrade"}
    },
    "aws-upi":{
      "optional":true,
      "prowJob":{"name":"release-openshift-ocp-installer-e2e-aws-upi-4.2"}
    }
  },
  "periodic":{
    "upgrade-aws-stable":{
      "cron":"@every 24h",
      "upgrade":true,
      "upgradeFromRelease":{
        "prerelease":{
          "version_bounds":{
            "lower":"4.2.0-0",
            "upper":"4.3.0-0"
          }
        }
      },
      "prowJob":{"name":"release-openshift-origin-installer-e2e-aws-upgrade-4.2-stable-to-4.2-nightly"}
    }
  }
}
