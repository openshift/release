{
  "name": "4.19.0-0.nightly",
  "to": "release",
  "message": "This release contains official image builds of all code in release-4.19 (main) branches, and is updated after those builds are synced to quay.io.",
  "mirrorPrefix": "4.19-art-latest",
  "expires": "336h",
  "maxUnreadyReleases": 1,
  "minCreationIntervalSeconds": 14400,
  "referenceMode": "source",
  "pullSecretName": "source",
  "alternateImageRepository": "quay.io/openshift-release-dev/dev-release",
  "alternateImageRepositorySecretName": "release-controller-quay-mirror-secret",
  "check": {
    "OCP and Origin images should match": {
      "consistentImages": {
        "parent": "4.19.0-0.ci"
      }
    }
  },
  "publish": {
    "issues": {
      "verifyIssues": {
        "previousReleaseTag": {
          "namespace": "ocp",
          "name": "release",
          "tag": "4.18.0-rc.0"
        }
      }
    },
    "tag": {
      "tagRef": {
        "name": "4.19"
      }
    }
  },
  "verify": {
    "aws-ovn-upgrade-4.19-micro-fips": {
      "maxRetries": 1,
      "prowJob": {
        "name": "periodic-ci-openshift-release-main-nightly-4.19-e2e-aws-ovn-upgrade-fips"
      },
      "upgrade": true
    },
    "azure-ovn-upgrade-4.19-micro": {
      "maxRetries": 1,
      "prowJob": {
        "name": "periodic-ci-openshift-release-main-ci-4.19-e2e-azure-ovn-upgrade"
      },
      "upgrade": true
    },
    "gcp-ovn-rt-upgrade-4.19-minor": {
      "maxRetries": 1,
      "prowJob": {
        "name": "periodic-ci-openshift-release-main-ci-4.19-upgrade-from-stable-4.18-e2e-gcp-ovn-rt-upgrade"
      },
      "upgrade": true,
      "upgradeFrom": "PreviousMinor"
    },
    "hypershift-aws-ovn-conformance-4.19": {
      "maxRetries": 2,
      "prowJob": {
        "name": "periodic-ci-openshift-hypershift-release-4.19-periodics-e2e-aws-ovn-conformance"
