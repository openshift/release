{
  "name":"4.7.0-0.okd",
  "to": "release",
  "mirrorPrefix": "4.7",
  "expires":"72h",
  "maxUnreadyReleases": 1,
  "minCreationIntervalSeconds": 3600,
  "publish":{
    "tag":{"tagRef":{"name":"4.7"}}
  },
  "verify":{
    "aws":{
      "prowJob":{"name":"periodic-ci-openshift-release-master-okd-4.7-e2e-aws"},
      "maxRetries": 2
    },
    "vsphere":{
      "optional": true,
      "maxRetries": 2,
      "prowJob":{"name":"periodic-ci-openshift-release-master-okd-4.7-e2e-vsphere"}
    },
    "upgrade":{
      "upgrade":true,
      "optional":true,
      "prowJob":{"name":"release-openshift-okd-installer-e2e-aws-upgrade"},
      "maxRetries": 2
    }
  }
}
