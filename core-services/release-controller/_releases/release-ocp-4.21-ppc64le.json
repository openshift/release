{
  "name": "4.21.0-0.nightly-ppc64le",
  "to": "release-ppc64le",
  "message": "This release contains official image builds of all ppc64le code in release-4.21 (master) branches, and is updated after those builds are synced to quay.io.",
  "mirrorPrefix": "4.21-art-latest-ppc64le",
  "expires": "336h",
  "maxUnreadyReleases": 1,
  "minCreationIntervalSeconds": 21600,
  "referenceMode": "source",
  "pullSecretName": "source",
  "alternateImageRepository": "quay.io/openshift-release-dev/dev-release",
  "alternateImageRepositorySecretName": "release-controller-quay-mirror-secret",
  "overrideCLIImage": "image-registry.openshift-image-registry.svc:5000/ocp/4.21-art-latest:cli",
  "check": {},
  "publish": {
    "tag": {
      "tagRef": {
        "name": "4.21"
      }
    }
  },
  "verify": {
    "e2e-ppc64le": {
      "optional": true,
      "prowJob": {
        "name": "periodic-ci-openshift-multiarch-master-nightly-4.21-ocp-e2e-ovn-remote-s2s-libvirt-ppc64le"
      }
    },
    "install-analysis-all": {
      "optional": true,
      "multiJobAnalysis": true,
      "prowJob": {
        "name": "periodic-ci-openshift-multiarch-master-nightly-4.21-install-analysis-all-ppc64le"
      }
    }
  }
}
