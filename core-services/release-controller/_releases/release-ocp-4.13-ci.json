{
  "name": "4.13.0-0.ci",
  "to": "release",
  "message": "This release contains CI image builds of all code in release-4.13 (master) branches, and is updated each time someone merges.",
  "mirrorPrefix": "4.13",
  "expires": "72h",
  "maxUnreadyReleases": 1,
  "minCreationIntervalSeconds": 21600,
  "pullSecretName": "source",
  "check": {},
  "publish": {
    "mirror-to-okd-scos": {
      "imageStreamRef": {
        "name": "scos-4.13",
        "namespace": "origin",
        "excludeTags": [
          "rhel-coreos",
          "rhel-coreos-extensions",
          "rhel-coreos-8",
          "rhel-coreos-8-extensions",
          "rhel-coreos-9",
          "rhel-coreos-9-extensions",
          "machine-config-operator",
          "machine-os-content",
          "contour",
          "contour-operator",
          "external-dns",
          "external-dns-operator",
          "installer",
          "installer-artifacts",
          "baremetal-installer",
          "openstack-installer",
          "upi-installer",
          "libvirt-installer",
          "ovirt-installer",
          "local-storage-static-provisioner",
          "sig-storage-local-static-provisioner",
          "ironic",
          "ironic-agent",
          "ironic-hardware-inventory-recorder",
          "ironic-inspector",
          "ironic-ipa-downloader",
          "cluster-samples-operator",
          "operator-marketplace",
          "cluster-update-keys",
          "machine-os-images",
          "windows-machine-config-bootstrapper-tests",
          "windows-machine-config-operator-bundle",
          "windows-machine-config-operator-test",
          "driver-toolkit",
          "driver-toolkit-tests"
        ]
      }
    },
    "mirror-to-origin": {
      "imageStreamRef": {
        "name": "4.13",
        "namespace": "origin",
        "excludeTags": [
          "rhel-coreos",
          "rhel-coreos-extensions",
          "rhel-coreos-8",
          "rhel-coreos-8-extensions",
          "rhel-coreos-9",
          "rhel-coreos-9-extensions",
          "machine-config-operator",
          "machine-os-content",
          "contour",
          "contour-operator",
          "external-dns",
          "external-dns-operator",
          "installer",
          "installer-artifacts",
          "baremetal-installer",
          "openstack-installer",
          "upi-installer",
          "libvirt-installer",
          "ovirt-installer",
          "local-storage-static-provisioner",
          "sig-storage-local-static-provisioner",
          "ironic",
          "ironic-agent",
          "ironic-hardware-inventory-recorder",
          "ironic-inspector",
          "ironic-ipa-downloader",
          "cluster-samples-operator",
          "operator-marketplace",
          "cluster-update-keys",
          "machine-os-images",
          "windows-machine-config-bootstrapper-tests",
          "windows-machine-config-operator-bundle",
          "windows-machine-config-operator-test",
          "driver-toolkit",
          "driver-toolkit-tests"
        ]
      }
    },
    "tag": {
      "tagRef": {
        "name": "4.13-ci"
      }
    }
  },
  "verify": {
    "aws-ovn-upgrade-4.13-micro": {
      "prowJob": {
        "name": "periodic-ci-openshift-release-master-ci-4.13-e2e-aws-ovn-upgrade"
      },
      "upgrade": true,
      "maxRetries": 3
    },
    "aws-ovn-upgrade-4.13-minor": {
      "prowJob": {
        "name": "periodic-ci-openshift-release-master-ci-4.13-upgrade-from-stable-4.12-e2e-aws-ovn-upgrade"
      },
      "upgrade": true,
      "maxRetries": 3,
      "upgradeFrom": "PreviousMinor"
    },
    "azure-sdn-upgrade-4.13-minor": {
      "prowJob": {
        "name": "periodic-ci-openshift-release-master-ci-4.13-upgrade-from-stable-4.12-e2e-azure-sdn-upgrade"
      },
      "upgrade": true,
      "maxRetries": 3,
      "upgradeFrom": "PreviousMinor"
    },
    "aws-sdn-serial": {
      "maxRetries": 3,
      "prowJob": {
        "name": "periodic-ci-openshift-release-master-ci-4.13-e2e-aws-sdn-serial"
      }
    },
    "gcp-sdn": {
      "optional": true,
      "prowJob": {
        "name": "periodic-ci-openshift-release-master-ci-4.13-e2e-gcp-sdn"
      }
    },
    "upgrade": {
      "optional": true,
      "prowJob": {
        "name": "periodic-ci-openshift-release-master-ci-4.13-e2e-gcp-sdn-upgrade"
      },
      "disabled": true,
      "upgrade": true
    },
    "upgrade-minor-aws-ovn": {
      "optional": true,
      "prowJob": {
        "name": "periodic-ci-openshift-release-master-ci-4.13-upgrade-from-stable-4.12-e2e-aws-ovn-upgrade"
      },
      "disabled": true,
      "upgrade": true,
      "upgradeFrom": "PreviousMinor"
    },
    "upgrade-minor-sdn": {
      "optional": true,
      "prowJob": {
        "name": "periodic-ci-openshift-release-master-ci-4.13-upgrade-from-stable-4.12-e2e-aws-sdn-upgrade"
      },
      "upgrade": true,
      "upgradeFrom": "PreviousMinor"
    }
  }
}