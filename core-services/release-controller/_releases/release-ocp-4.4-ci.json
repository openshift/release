{
  "name":"4.4.0-0.ci",
  "to": "release",
  "message": "This release contains CI image builds of all code in release-4.4 (master) branches, and is updated each time someone merges.",
  "mirrorPrefix": "4.4",
  "expires":"96h",
  "maxUnreadyReleases": 2,
  "minCreationIntervalSeconds": 2400,
  "pullSecretName": "source",
  "publish":{
    "mirror-to-origin":{
      "imageStreamRef":{
        "namespace": "origin",
        "name": "4.4",
        "excludeTags": [
          "machine-os-content",
          "machine-config-operator",
          "machine-config-operator-rpms",
          "installer",
          "installer-artifacts",
          "baremetal-installer",
          "openstack-installer",
          "upi-installer",
          "libvirt-installer",
          "baremetal-machine-controllers",
          "baremetal-operator",
          "sig-storage-local-static-provisioner",
          "ironic",
          "ironic-inspector",
          "ironic-ipa-downloader",
          "ironic-machine-os-downloader",
          "ironic-static-ip-manager"
        ]
      }
    },
    "tag":{"tagRef":{"name":"4.4-ci"}}
  },
  "verify":{
    "gcp":{
      "prowJob":{"name":"release-openshift-origin-installer-e2e-gcp-4.4"}
    },
    "gcp-serial":{
      "prowJob":{"name":"release-openshift-origin-installer-e2e-gcp-serial-4.4"}
    },
    "upgrade":{
      "upgrade":true,
      "optional":true,
      "prowJob":{"name":"release-openshift-origin-installer-e2e-aws-upgrade"}
    },
    "upgrade-minor":{
      "upgrade":true,
      "upgradeFrom": "PreviousMinor",
      "optional":false,
      "prowJob":{"name":"release-openshift-origin-installer-e2e-aws-upgrade"}
    }
  },
  "periodic":{
    "upgrade-aws-fips":{
      "cron":"@every 12h",
      "upgrade":true,
      "prowJob":{"name":"release-openshift-origin-installer-e2e-aws-upgrade-fips-4.4"}
    },
    "upgrade-aws-stable":{
      "cron":"@every 48h",
      "upgrade":true,
      "upgradeFromRelease":{
        "prerelease":{
          "version_bounds":{
            "lower":"4.4.0-0",
            "upper":"4.5.0-0"
          }
        }
      },
      "prowJob":{"name":"release-openshift-origin-installer-e2e-aws-upgrade-4.4-stable-to-4.4-ci"}
    },
    "upgrade-aws-rollback-stable":{
      "cron":"@every 24h",
      "upgrade":true,
      "upgradeFromRelease":{
        "prerelease":{
          "version_bounds":{
            "lower":"4.4.0-0",
            "upper":"4.5.0-0"
          }
        }
      },
      "prowJob":{"name":"release-openshift-origin-installer-e2e-aws-upgrade-rollback-4.4-to-4.4"}
    },
    "upgrade-aws-rollback":{
      "cron":"@every 24h",
      "upgrade":true,
      "prowJob":{"name":"release-openshift-origin-installer-e2e-aws-upgrade-rollback-4.4"}
    },
    "upgrade-gcp":{
      "cron":"@every 48h",
      "upgrade":true,
      "prowJob":{"name":"release-openshift-origin-installer-e2e-gcp-upgrade-4.4"}
    },
    "upgrade-azure":{
      "cron":"@every 6h",
      "upgrade":true,
      "prowJob":{"name":"release-openshift-origin-installer-e2e-azure-upgrade-4.4"}
    }
  }
}
