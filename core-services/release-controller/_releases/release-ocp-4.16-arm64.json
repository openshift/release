{
  "name": "4.16.0-0.nightly-arm64",
  "to": "release-arm64",
  "message": "This release contains official image builds of all arm64 code in release-4.16 (master) branches, and is updated after those builds are synced to quay.io.",
  "mirrorPrefix": "4.16-art-latest-arm64",
  "expires": "336h",
  "maxUnreadyReleases": 1,
  "minCreationIntervalSeconds": 28800,
  "referenceMode": "source",
  "pullSecretName": "source",
  "alternateImageRepository": "quay.io/openshift-release-dev/dev-release",
  "alternateImageRepositorySecretName": "release-controller-quay-mirror-secret",
  "overrideCLIImage": "image-registry.openshift-image-registry.svc:5000/ocp/4.16-art-latest:cli",
  "check": {},
  "publish": {
    "tag": {
      "tagRef": {
        "name": "4.16"
      }
    }
  },
  "verify": {
    "aws-ovn-arm64": {
      "maxRetries": 3,
      "prowJob": {
        "name": "periodic-ci-openshift-multiarch-master-nightly-4.16-ocp-e2e-aws-ovn-arm64"
      }
    },
    "ovn-upgrade": {
      "maxRetries": 3,
      "prowJob": {
        "name": "periodic-ci-openshift-multiarch-master-nightly-4.16-ocp-e2e-upgrade-aws-ovn-arm64"
      },
      "upgrade": true
    }
  }
}
