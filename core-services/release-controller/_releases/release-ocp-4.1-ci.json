{
  "name":"4.1.0-0.ci",
  "to": "release",
  "message": "This release contains CI image builds of all code in release-4.1 branches, and is updated each time someone merges.",
  "mirrorPrefix": "4.1",
  "expires":"72h",
  "maxUnreadyReleases": 2,
  "minCreationIntervalSeconds": 1800,
  "pullSecretName": "source",
  "publish":{
    "mirror-to-origin":{
      "imageStreamRef":{
        "namespace": "origin",
        "name": "4.1",
        "excludeTags": [
          "machine-os-content",
          "ironic-ipa-downloader",
          "ironic-machine-os-downloader"
        ]
      }
    },
    "tag":{"tagRef":{"name":"4.1-ci"}}
  },
  "verify":{
    "aws":{
      "prowJob":{"name":"release-openshift-origin-installer-e2e-aws-4.1"}
    },
    "aws-serial":{
      "prowJob":{"name":"release-openshift-origin-installer-e2e-aws-serial-4.1"}
    },
    "upgrade":{
      "upgrade":true,
      "optional":true,
      "prowJob":{"name":"release-openshift-origin-installer-e2e-aws-upgrade"}
    }
  },
  "periodic":{
    "upgrade-aws-rollback":{
      "cron":"@every 24h",
      "upgrade":true,
      "prowJob":{"name":"release-openshift-origin-installer-e2e-aws-upgrade-rollback-4.1"}
    }
  }
}
