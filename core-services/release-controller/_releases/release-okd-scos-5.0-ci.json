{
  "name": "5.0.0-0.okd-scos",
  "to": "release-scos",
  "mirrorPrefix": "5.0-okd-scos",
  "expires": "72h",
  "maxUnreadyReleases": 1,
  "minCreationIntervalSeconds": 43200,
  "check": {},
  "publish": {
    "tag": {
      "tagRef": {
        "name": "scos-5.0-ci"
      }
    }
  },
  "verify": {
    "aws": {
      "maxRetries": 2,
      "prowJob": {
        "name": "periodic-ci-openshift-release-master-okd-scos-5.0-e2e-aws-ovn"
      }
    },
    "aws-upgrade":{
      "maxRetries": 2,
      "optional": true,
      "prowJob": {
        "name": "periodic-ci-openshift-release-master-okd-scos-5.0-e2e-aws-ovn-upgrade"
      },
      "upgrade": true
    },
    "aws-upgrade-minor": {
      "optional": true,
      "prowJob": {
        "name": "periodic-ci-openshift-release-master-okd-scos-5.0-upgrade-from-okd-scos-4.22-e2e-aws-ovn-upgrade"
      },
      "upgrade": true,
      "upgradeFromRelease": {
        "candidate": {
          "stream": "okd",
          "version": "4.22"
        }
      }
    }
  }
}
