{
  "name":"4-stable",
  "as": "Stable",
  "message": "All amd64 official image versions in 4.y.z are tracked here",
  "overrideCLIImage": "image-registry.openshift-image-registry.svc:5000/ocp/4.19:cli",
  "expires":"72h",
  "pullSecretName": "source",
  "publish":{},
  "verify":{
    "upgrade":{
      "upgrade":true,
      "maxRetries": 2,
      "prowJob":{"name":"periodic-ci-openshift-release-master-stable-4.y-e2e-aws-ovn-upgrade"}
    },
    "upgrade-minor":{
      "upgrade":true,
      "maxRetries": 2,
      "upgradeFrom": "PreviousMinor",
      "prowJob":{"name":"periodic-ci-openshift-release-master-stable-4.y-e2e-aws-ovn-upgrade"}
    }
  },
  "upgrade": {
    "aws": {
      "prowJob":{"name":"periodic-ci-openshift-release-master-stable-4.y-e2e-aws-ovn-upgrade"}
    },
    "azure": {
      "prowJob":{"name":"periodic-ci-openshift-release-master-stable-4.y-e2e-azure-ovn-upgrade"}
    },
    "gcp": {
      "prowJob":{"name":"periodic-ci-openshift-release-master-stable-4.y-e2e-gcp-upgrade"}
    }
  }
}
