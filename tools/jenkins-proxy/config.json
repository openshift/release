{
    "proxy_auth": {
        "user": "openshift-ci-robot",
        "token_file": "/etc/jenkins/basic"
    },
    "masters": [
        {
            "url": "https://ci.openshift.redhat.com/jenkins",
            "auth": {
                "basic_auth": {
                    "user": "openshift-ci-robot",
                    "token_file": "/etc/jenkins/basic"
