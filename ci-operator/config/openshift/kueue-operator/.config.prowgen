multi_arch: yes
slack_reporter:
- channel: "#forum-node-jira"
  job_states_to_report:
  - failure
  - error
  report_template: ':failed: Job *{{.Spec.Job}}* ended with *{{.Status.State}}*. <{{.Status.URL}}|View logs> {{end}}'
  job_name_patterns:
  - ^test-e2e-.*
  excluded_job_patterns: 
  - ^pull-.*
