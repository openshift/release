{
  "name":"4.1.0-0.ci",
  "to": "release",
  "message": "After merging to master, CI images are built and a release is created",
  "mirrorPrefix": "4.1",
  "expires":"72h",
  "pullSecretName": "source",
  "publish":{
    "mirror-to-origin":{
      "imageStreamRef":{
        "namespace": "origin",
        "name": "4.1"
      }
    }
  },
  "verify":{
    "aws":{
      "prowJob":{"name":"release-openshift-origin-installer-e2e-aws-4.1"}
    },
    "aws-serial":{
      "prowJob":{"name":"release-openshift-origin-installer-e2e-aws-serial-4.1"}
    },
    "upgrade":{
      "upgrade":true,
      "optional":true,
      "prowJob":{"name":"release-openshift-origin-installer-e2e-aws-upgrade"}
    }
  }
}